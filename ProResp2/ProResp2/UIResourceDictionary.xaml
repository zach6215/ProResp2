<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Button" x:Key="BasicButton">
        <Setter Property="Padding" Value="5"/>
        <Setter Property="Margin" Value="5"/>
    </Style>
    <Style TargetType="TextBlock" x:Key="BasicTextBlock">
        <Setter Property="Padding" Value="5"/>
        <Setter Property="Margin" Value="5,5,5,0"/>
    </Style>
    <Style TargetType="ListBox" x:Key="BasicListBox">
        <Setter Property="Padding" Value="5"/>
        <Setter Property="Margin" Value="5,0,5,0"/>
    </Style>
    <Style TargetType="CheckBox" x:Key="ValveCheckBox">
        <Setter Property="Margin" Value="5"/>
    </Style>
    <Style TargetType="GroupBox" x:Key="BasicGroupBox">
        <Setter Property="Margin" Value="5"/>
        <Setter Property="Padding" Value="5"/>
    </Style>
</ResourceDictionary>